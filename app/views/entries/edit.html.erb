<% @page_title = "ブログ記事の編集" %>
<h1 class="member-entries"><%= @page_title %></h1>

<div class="entry-edit"><%= link_to "記事の表示に戻る", @entry %></div>

<%= form_for @entry do |form| %>
  <%= render "form", form: form %>
  <div class="btn-set"><%= form.submit '更新', class: 'btn btn-primary' %></div>
<% end %>
